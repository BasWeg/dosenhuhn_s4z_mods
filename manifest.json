{
    "manifest_version": 1,
    "name": "DosenHuhn S4Z MOD",
    "description": "Sauce4Zwift MODs from DosenHuhn",
    "version": "0.0.1",
    "web_root": "pages",
    "windows": [{
        "file": "pages/wbal_update.html",
        "id": "dosenhuhn-wbal-update",
        "name": "DH MOD - wbal update",
        "description": "Mod for Sauce to update team Wbal data with zwitracing.app values",
        "always_visible": true,
        "overlay": true,
        "frame": false,
        "default_bounds": {
            "width": 300,
            "height": 300,
            "x": 100,
            "y": 100
        }
    },	
    {
        "file": "pages/groups.html",
        "id": "dosenhuhn-groups",
        "name": "DH MOD - groups",
        "description": "change of group window to show (watching) team-badges",
        "always_visible": true,
        "overlay": true,
        "frame": false,
        "default_bounds": {
            "width": 300,
            "height": 800,
            "x": -300,
            "y": 100
        }
    }]
}
