{
    "manifest_version": 1,
    "name": "DosenHuhn MODs for S4Z",
    "description": "A MOD collection for S4Z",
    "author": "Bastian Wegener",
    "website_url": "https://www.wattfabrik.org",
    "version": "0.0.3",
    "web_root": "pages",
    "windows": [{
        "file": "pages/wbal_update.html",
        "id": "dosenhuhn-wbal-update",
        "name": "DH MOD - wPrime and cp update",
        "description": "Update wPrime/cp data from zwiftracing.app or json",
        "always_visible": true,
        "overlay": true,
        "frame": false,
        "default_bounds": {
            "width": 500,
            "height": 700,
            "x": 50,
            "y": 50
        }
    },     
    {
        "file": "pages/groups.html",
        "id": "dosenhuhn-groups",
        "name": "DH MOD - groups",
        "description": "change of group window to show (watching) team-badges",
        "always_visible": true,
        "overlay": true,
        "frame": false,
        "default_bounds": {
            "width": 300,
            "height": 800,
            "x": -300,
            "y": 100
        }
    }]
}
